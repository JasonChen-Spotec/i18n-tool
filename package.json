    "server": "node ./app/bin/www --exec babel-node -e js",
    "buildTest": "cross-env NODE_ENV=production API_ENV=development babel-node ./build/webpack.pro.js",
    "build": "cross-env NODE_ENV=production API_ENV=production babel-node ./build/webpack.pro.js",
    "eslint": "eslint --cache --quiet --ext .js,.jsx ./src",
    "eslint:fix": "eslint --fix --quiet --cache --ext .js,.jsx --format=pretty ./src",
    "stylelint": "stylelint 'src/**/*.less'",
    "stylelint:fix": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint": "npm run eslint && npm run stylelint",
    "build:i18n": "cross-env NODE_ENV=production babel-node build/i18n/index.js",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --config=\"./jest.config.js\"",
    "test": "cross-env NODE_ENV=test jest --config=\"./jest.config.js\""
  },
